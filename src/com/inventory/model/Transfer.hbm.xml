<?xml version="1.0" encoding="UTF-8"?>  
    <!DOCTYPE hibernate-mapping PUBLIC  
            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.inventory.model">
	<class name="Transfer" table="Transfer">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="fromBranchId" column="fromBranchId" type="long" />
		<property name="destinationBranchId" column="destinationBranchId" type="long" />
		<property name="fromStockid" column="fromStockid" type="long" />
		<property name="quantity" column="quantity" />
		<property name="status" column="status" />
		<property name="creattime" column="creattime" />
	</class>
</hibernate-mapping>  